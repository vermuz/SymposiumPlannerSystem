            <groupId>org.mule.modules</groupId>
            <artifactId>mule-module-builders</artifactId>
            <version>${version}</version>
            <scope>test</scope>
        </dependency>
       	<dependency>
            <groupId>org.mule.modules</groupId>
            <artifactId>mule-module-spring-config</artifactId>
            <version>${version}</version>
        </dependency>
    </dependencies>
